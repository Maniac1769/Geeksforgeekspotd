class Solution:  
  def longestCommonPrefix((arr,n):
    i,fail,pfix=0,0,''
        while i<len(min(arr)):
            for j in range(1,n):
                #print(i,j,arr[0][i])
                if arr[0][i]!=arr[j][i]:
                    fail=1
                    break
            if fail==1: break 
            else:
                pfix=pfix+arr[0][i]
                i+=1
        if len(pfix)==0: return (-1)
        else: return pfix
if __name__=='__main__':
